steal('can/control', 'can/view/ejs', './views/init.ejs', function(Control, EJS, initView){
    /**
     * @class <%= name %>
     */
    return Control({
        /** @Static */
        defaults : {}
      },
      /** @Prototype */
      {
        init : function(){
          this.element.html(initView({
            message: "Hello World"
          }));
        }
      });
});