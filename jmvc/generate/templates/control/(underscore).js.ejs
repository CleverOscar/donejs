steal('can/control', './init.ejs', function(Control, initView){
    /**
     * @class <%= name %>
     */
    return Control(
    /** @Static */
    {
        defaults : {}
      },
      /** @Prototype */
      {
        init : function(){
          this.element.html(initView({
            message: "Hello World from <%= name %>"
          }));
        }
      });
});