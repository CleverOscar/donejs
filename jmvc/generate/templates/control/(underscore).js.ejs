steal('can/control', 'can/view/ejs', './views/init.ejs', function(Control, EJS){

    /**
     * @class <%= name %>
     */
    Control('<%= name %>',
    /** @Static */
    {
      defaults : {}
    },
    /** @Prototype */
    {
      init : function(){
        this.element.html(can.view("//<%= path %>/<%= underscore %>/views/init.ejs",{
          message: "Hello World"
        }));
      }
    })

    return <%= name %>;
});