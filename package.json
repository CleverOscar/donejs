{
  "name": "donejs",
  "version": "0.3.0",
  "description": "Your app is done",
  "main": "index.js",
  "bin": {
    "donejs": "./bin/donejs"
  },
  "scripts": {
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint lib/. bin/donejs test/. --config",
    "mocha": "mocha test/ --timeout 120000",
    "verify": "echo \"NPM script ran\"",
    "document:watch": "documentjs --watch",
    "document": "documentjs -f && rm -rf site/img && cp -R docs/img site/img",
    "publish": "git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donejs/donejs.git"
  },
  "keywords": [
    "CanJS",
    "StealJS",
    "DoneJS"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/donejs/donejs/issues"
  },
  "homepage": "https://github.com/donejs/donejs",
  "devDependencies": {
    "documentjs": "^0.3.0-pre.4",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "mkdirp": "^0.5.1",
    "npm": "^2.13.1",
    "q": "^1.4.1",
    "yeoman-environment": "^1.2.7"
  },
  "directories": {
    "doc": "docs"
  },
  "donejs": {
    "dependencies": {
      "can": "~2.3.0-pre.9",
      "can-connect": "~0.2.2",
      "can-ssr": "^0.5.7",
      "done-autorender": "^0.3.1",
      "done-component": "^0.2.0",
      "done-css": "~1.1.7",
      "generator-donejs": "^0.2.9",
      "jquery": "~2.1.4",
      "steal": "^0.11.0-pre.8",
      "yeoman-environment": "~1.2.7"
    },
    "devDependencies": {
      "documentjs": "^0.3.0",
      "donejs-deploy": "^0.1.6",
      "funcunit": "~3.0.0",
      "steal-qunit": "^0.1.1",
      "steal-tools": "^0.11.0-pre.11",
      "testee": "^0.2.0"
    }
  }
}
